





  public static void miniMaxSum(List<Integer> arr) {
    // Write your code here

        Collections.sort(arr);
        int l= arr.size();
        long min=0;
        long max=0;
        for(int i=0;i<4;i++){
            min+=arr.get(i);
        }
        for(int i=l-1;i>=l-4;i--){
            max+=arr.get(i);
        }
        System.out.println(min+" "+max);

}
